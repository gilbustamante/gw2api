<%- layout("layouts/boilerplate") %>
<div class="d-flex justify-content-between">
  <div>
    <h1>Watchlist</h1>
  </div>
  <div>
    <button class="btn">Update</button>
  </div>
</div>
<hr>
<form action="/market/watchlist" method="POST">
